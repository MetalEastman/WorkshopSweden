                            <ul class="nav navbar-nav nav-content-align">
                                <li><a id="a-padding1" class="nav-li-top" href="@Url.Action("Index", "About")">OM OSS</a></li>
                                <li><a id="a-padding2" class="nav-li-top" href="@Url.Action("Index", "Contact")">KONTAKT</a></li>
                                <li><a id="a-padding7" class="nav-li-top" href="@Url.Action("Index", "English")">ENGLISH</a></li>

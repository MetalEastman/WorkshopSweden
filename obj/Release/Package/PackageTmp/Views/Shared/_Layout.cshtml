                <div class="nav-inner">
                    <div class="nav-inner-title">
                        <a class="navbar-brand navbar-title-link" href="@Url.Action("Index", "Home")"><span class="navbar-title-top">Workshop</span></a>
                        @*@Html.ActionLink("W", "Index", "Home", new { area = "" }, new { @class = "navbar-brand navbar-title-W navbar-title-top" })
